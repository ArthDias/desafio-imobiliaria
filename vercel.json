{
    "version": 2,
    "builds": [
      {
        "src": "api/api-usuario/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "api/api-fake/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/api/api-usuario/(.*)", "dest": "/api/api-usuario/server.js" },
      { "src": "/api/api-fake/(.*)", "dest": "/api/api-fake/index.js" },
      { "src": "/(.*)", "dest": "/dist/$1" }
    ]
  }
  